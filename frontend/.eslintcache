ktop\\Cachazo_Mytinerari\\frontend\\src\\components\\Cities.js",["83","84"],"import React , { useEffect, useState } from 'react'\r\nimport {Link} from 'react-router-dom'\r\nimport Nav from \"./Nav\"\r\nimport Spiner from  \"./Spiner\"\r\nimport Footer from \"./Footer\"\r\nimport citiesAction from \"../redux/actions/citiesAction\"\r\nimport {connect} from 'react-redux'\r\n// llamo a la propiedad a traves de las props que la obtengo de abajo del dispatch\r\nconst Cities = (props) => {\r\n    useEffect(() => {\r\n        props.callCities()     \r\n    },[]) \r\n\r\n    const [loaded , setLoaded]=useState(false)\r\n\r\n    useEffect(() => {\r\n        return setLoaded    \r\n    })\r\n\r\n    if(loaded === false ){\r\n        return <Spiner/>\r\n    }\r\n    return (\r\n        <div className=\"contenedorCity\"><Nav/>\r\n                <div className=\"titleCities\">\r\n                    <h3>CITIES</h3>\r\n                </div>\r\n            \r\n                <div className = \"inputBox\"><input type= \"text\" className = \"inputIn\" placeholder = \"SearchCities\" onChange = {(e) => props.leerInput(e.target.value)}></input></div> \r\n            \r\n                {props.filterCities.length === 0 \r\n                \r\n                ? <div className = \"noCities\"><p>SORRY,NO CITIES WITH THAT NAME WERE FOUND<br></br>PLEASE TRY AGAIN</p></div>\r\n                \r\n                : props.filterCities.map(({cityName ,cityPic,_id}) => { \r\n                    return(\r\n                    <div key={_id} className = \"cities\">\r\n                        <div className = \"ciudadesImagen\" style = {{backgroundImage: `url('${cityPic}')`}}>\r\n                            <Link to = {`/cities/${_id}`}><h3 className = \"ciudadesNombre\">{cityName}</h3></Link> \r\n                        </div>\r\n                   </div>\r\n                   )\r\n                })}  \r\n                <Footer/>\r\n        </div>       \r\n    )\r\n}\r\nconst mapStateToProps = state  => {\r\n    return {\r\n        allCities: state.citiesReducer.cities,\r\n        filterCities:state.citiesReducer.filterCities\r\n    }\r\n}\r\n// el dispatch mapea las actions que tengo , indicando la accion (componente )y la propiedad a realizar\r\nconst mapDispatchToProps = {\r\n    callCities : citiesAction.allCities,\r\n    leerInput: citiesAction.filtrarCities\r\n}\r\n\r\nexport default connect (mapStateToProps,mapDispatchToProps)(Cities)\r\n\r\n\r\n\r\n\r\n","C:\\Users\\Lucas\\Desktop\\Cachazo_Mytinerari\\frontend\\src\\components\\Nav.js",[],"C:\\Users\\Lucas\\Desktop\\Cachazo_Mytinerari\\frontend\\src\\components\\CallToAction.js",[],"C:\\Users\\Lucas\\Desktop\\Cachazo_Mytinerari\\frontend\\src\\components\\Main.js",[],"C:\\Users\\Lucas\\Desktop\\Cachazo_Mytinerari\\frontend\\src\\components\\Container.js",[],"C:\\Users\\Lucas\\Desktop\\Cachazo_Mytinerari\\frontend\\src\\components\\City.js",["85"],"import React, { useEffect, useState } from 'react'\r\nimport Nav from \"./Nav\"\r\nimport Itinerary from './Itinerary'\r\nimport {connect} from 'react-redux'\r\nimport itinerariesAction from '../redux/actions/itinerariesAction'\r\nimport { Link } from 'react-router-dom'\r\n\r\nexport const  City = (props) => {\r\n    const [city , setCity] = useState([])\r\n    const id = props.match.params.id\r\n\r\n\r\n    useEffect (() => {\r\n        props.callItineraries(id)\r\n        const city = props.cities.filter(city =>{\r\n            return ( \r\n                city._id === id\r\n            )\r\n        })\r\n        setCity(city[0])\r\n    },[])\r\n\r\n    \r\n    if( props.cities.length !== 0 ) {\r\n        return (\r\n        <>    \r\n        <div className = \"elementCity\">\r\n            <Nav/>\r\n            <h4 className = \"titleCity\">{city.cityName}</h4 >\r\n            <div>\r\n                 <div className = \"pikCity\" style={{backgroundImage : `url('${city.cityPic}')`}}/>\r\n                    {props.allItineraries.length === 0 \r\n    